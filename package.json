{
    "name": "at-design-system",
    "version": "1.0.0",
    "description": "Design system using Style Dictionary v5",
    "type": "module",
    "scripts": {
        "build": "npm run build:light && npm run build:dark",
        "build:light": "style-dictionary build --config sd.config.light.mjs",
        "build:dark": "style-dictionary build --config sd.config.dark.mjs",
        "build:tokens": "npm run build",
        "clean": "rm -rf build"
    },
    "keywords": [
        "design-tokens",
        "style-dictionary"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "style-dictionary": "^5.0.0"
    }
}